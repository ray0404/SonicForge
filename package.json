{
  "name": "sonic-forge",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/sonic-forge.umd.cjs",
  "module": "./dist/sonic-forge.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/sonic-forge.js",
      "require": "./dist/sonic-forge.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "bundle-worklets": "node scripts/bundle-worklets.cjs",
    "dev": "vite build --watch",
    "build": "npm run bundle-worklets && tsc && vite build",
    "lint": "eslint . --ext ts --max-warnings 0",
    "test": "vitest"
  },
  "dependencies": {
    "standardized-audio-context": "^25.3.77"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jsdom": "^27.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^4.0.15"
  }
}
